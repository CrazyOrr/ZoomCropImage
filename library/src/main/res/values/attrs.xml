<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CropImage">
        <attr name="zci_borderWidth" format="dimension" />
        <attr name="zci_borderColor" format="color" />
        <attr name="zci_shaderColor" format="color" />
        <attr name="zci_scaleMax" format="float" />
        <attr name="zci_scaleMin" format="float" />
        <attr name="zci_cropShape">
            <flag name="rectangle" value="0" />
            <flag name="oval" value="1" />
        </attr>
    </declare-styleable>
    
</resources>